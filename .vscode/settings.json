{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/coverge": true
  },
  "typescript.referencesCodeLens.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "tslint.ignoreDefinitionFiles": false,
  "tslint.autoFixOnSave": true,
  "tslint.exclude": [
    "**/node_modules/**/*",
    "**/test/**/*",
    "**/rollup.config.ts"
  ],
  "cSpell.words": [
    "csrf",
    "definitelytyped",
    "promisified",
    "ueqt",
    "vuets",
    "ueqtxu",
    "gmail",
    "prebuild",
    "typedoc",
    "precommit",
    "lcov",
    "camelcase",
    "truthy",
    "instantiable",
    "sourcemap",
    "endregion"
  ]
}